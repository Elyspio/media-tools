	"components": {
		"schemas": {
			"AppArch": {
				"type": "string",
				"enum": [
					"Win32",
					"Win64",
					"LinuxDeb",
					"LinuxRpm",
					"LinuxSnap"
				]
			},
			"AppMetadata": {
				"type": "object",
				"additionalProperties": false,
				"required": [
					"arch",
					"name",
					"version"
				],
				"properties": {
					"name": {
						"type": "string"
					},
					"version": {
						"$ref": "#/components/schemas/AppVersion"
					},
					"arch": {
						"$ref": "#/components/schemas/AppArch"
					}
				}
			},
			"AppVersion": {
				"type": "object",
				"additionalProperties": false,
				"required": [
					"major",
					"minor",
					"raw",
					"revision"
				],
				"properties": {
					"minor": {
						"type": "integer",
						"format": "int32"
					},
					"major": {
						"type": "integer",
						"format": "int32"
					},
					"revision": {
						"type": "integer",
						"format": "int32"
					},
					"raw": {
						"type": "string",
						"readOnly": true
					}
				}
			},
			"IResult": {
				"type": "object",
				"additionalProperties": false
			}
		}
